%section.text-center.section-padding
  .container
    .row
      .col-md-12.text-right.navicon
        %a#nav-toggle.nav_slide_button{:href => "#"}
          %span
    .row
      .col-md-12
        %h1.arrow= @client.user.username
    .row
      .team-wrapper
        #teamSlider
          %ul.slides
          - @client.user_media_feed.each_slice(4) do |slice|           
            %li
            - slice.each do |media_item|
              .col-md-3.wp5
                %img{:alt => "Team Member", :src => "#{media_item.images.thumbnail.url}"}

